generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Todo {
  id Int @id @default(autoincrement())
  title String @db.VarChar(40)
  content String @db.VarChar(100)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
